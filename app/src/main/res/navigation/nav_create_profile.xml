<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_create_profile"
    app:startDestination="@id/enter_mobile_fragment">


    <fragment
        android:id="@+id/enter_mobile_fragment"
        android:name="com.provider.unobridge.ui.fragments.auth.EnterMobileFragment"
        android:label="enter_mobile_fragment"
        tools:layout="@layout/enter_mobile_fragment" />

    <fragment
        android:id="@+id/otp_fragment"
        android:name="com.provider.unobridge.ui.fragments.auth.OtpVerificationFragment"
        android:label="otp_fragment"
        tools:layout="@layout/otp_fragment" />

    <fragment
        android:id="@+id/user_details_fragment"
        android:name="com.provider.unobridge.ui.fragments.profile.UserDetailsFragment"
        android:label="user_details_fragment"
        tools:layout="@layout/user_details_fragment" />

    <fragment
        android:id="@+id/user_type_fragment"
        android:name="com.provider.unobridge.ui.fragments.profile.UserTypeFragment"
        android:label="user_type_fragment"
